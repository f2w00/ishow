{
    "name": "ua.client",
    "productName": "ua.client",
    "version": "1.0.0",
    "description": "This client solve any problem with plugins in any system for 'Internet of Things'",
    "type": "commonjs",
    "scripts": {
        "dev": "nodemon --exec node ua.servant/ua.servant.js --delay 2",
        "compile": "nodemon -e ts --exec npx ttsc",
        "pack": "webpack"
    },
    "keywords": [],
    "author": {
        "name": "f2w00",
        "email": "109409603+f2w00@users.noreply.github.com"
    },
    "license": "MIT",
    "dependencies": {
        "koa": "^2.14.1",
        "koa-body": "^6.0.1",
        "koa-router": "^12.0.0",
        "node-gyp": "^9.3.1",
        "node-opcua": "^2.96.0",
        "node-opcua-pki": "^4.2.1",
        "pg-hstore": "^2.3.4",
        "reflect-metadata": "^0.1.13",
        "typia": "^3.4.22"
    },
    "devDependencies": {
        "@types/koa-router": "^7.4.4",
        "@types/node": "^18.15.3",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "ttypescript": "^1.5.15",
        "typescript": "^4.9.5",
        "webpack": "^5.87.0",
        "webpack-cli": "^5.1.4"
    },
    "uniPlugin": {
        "version": "1",
        "storage": "\\ua.client\\package.json",
        "main": "\\ua.client\\extension.js",
        "worker": "\\ua.client\\ua.servant\\uaclient.js",
        "render": {
            "extraButtons": [
                {
                    "name": "uaclient",
                    "loc": "leftSide",
                    "iconPath": "",
                    "mainViewPath": "render",
                    "sideViewPath": "side",
                    "clickToSendInWindow": null,
                    "clickToSendInMain": "render:leftSide.click-uaclient"
                }
            ]
        },
        "engine": "兼容客户端版本",
        "identifier": {
            "id": "uaclient",
            "uuid": null
        },
        "onEvents": [
            "project:activate.opcua",
            "render:leftSide.click-uaclient"
        ],
        "projectExtend": [
            "opcua"
        ],
        "defaultStart": true
    }
}
